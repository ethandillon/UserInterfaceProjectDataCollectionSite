{
  "name": "movie-recommender-ab-test",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_TMDB_API_KEY": "@tmdb_api_key",
    "VITE_SHEET_BEST_ENDPOINT": "@sheet_best_endpoint"
  }
}